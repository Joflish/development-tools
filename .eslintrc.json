{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier", "stylelint-config-standard",
    "stylelint-prettier/recommended",
    "stylelint-disable-next-line prettier/prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": ["true"],
        "camelcase": "error",
        "eqeqeq": ["error", "always"]
    }
}
